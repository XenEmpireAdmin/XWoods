<Resources>

  



  <ThingDef ParentName="ResourceVerbBase">
    <defName>BlackWood</defName>
    <label>blackwood</label>
    <description>Dark Wood from trees or other fibrous plants. Useful for building structures, weapons, and even simple prosthetics. A raw stick of wood can be used as a weapon in a pinch.</description>
    <graphicData>
      <texPath>Things/Item/Resource/WoodLog</texPath>
	  <color>(64,64,68)</color>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <equippedAngleOffset>-20</equippedAngleOffset>
    <soundInteract>Wood_Drop</soundInteract>
    <soundDrop>Wood_Drop</soundDrop>
    <statBases>
      <MarketValue>1.4</MarketValue>
      <MaxHitPoints>150</MaxHitPoints>
      <Mass>0.4</Mass>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>1.5</DeteriorationRate>
      <MeleeWeapon_DamageAmount>8</MeleeWeapon_DamageAmount>
      <MeleeWeapon_Cooldown>2.0</MeleeWeapon_Cooldown>
      <SharpDamageMultiplier>0.3</SharpDamageMultiplier>
      <BluntDamageMultiplier>0.8</BluntDamageMultiplier>
    </statBases>
    <stuffProps>
      <stuffAdjective>blackwood</stuffAdjective>
      <categories>
        <li>Woody</li>
      </categories>
      <commonality>1.00</commonality>
      <color>(64,64,68)</color>
      <constructEffect>ConstructWood</constructEffect>
      <appearance>Planks</appearance>
      <soundImpactStuff>BulletImpactWood</soundImpactStuff>
      <soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
      <statOffsets>
        <Beauty>2</Beauty>
      </statOffsets>
      <statFactors>
        <MaxHitPoints>1.0</MaxHitPoints>
        <Beauty>2.5</Beauty>
        <Flammability>1</Flammability>
        <WorkToMake>0.75</WorkToMake>
				<WorkToBuild>0.75</WorkToBuild>
        <DoorOpenSpeed>1.2</DoorOpenSpeed>
        <MeleeWeapon_Cooldown>0.9</MeleeWeapon_Cooldown>
      </statFactors>
    </stuffProps>
    <isBodyPartOrImplant>true</isBodyPartOrImplant>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <techHediffsTags>
      <li>Poor</li>
    </techHediffsTags>
    <verbs>
      <li>
        <verbClass>Verb_MeleeAttack</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <meleeDamageDef>Blunt</meleeDamageDef>
